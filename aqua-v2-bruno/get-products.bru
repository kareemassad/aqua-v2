meta {
  name: Get Products
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/api/products?page=1&limit=10&search=&store_id={{storeId}}
  auth: bearer
}

headers {
  Content-Type: application/json
}

auth:bearer {
  token: {{authToken}}
}

vars:pre-request {
  baseUrl: http://localhost:3000
  storeId: // Add the store ID here
  authToken: // Add your auth token here
}