meta {
  name: Get User
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/api/user/{{userId}}/dashboard
  body: none
  auth: bearer
}

headers {
  Content-Type: application/json
}

auth:bearer {
  token: {{authToken}}
}

vars:pre-request {
  baseUrl: http://localhost:3000
  userId: // Add the user ID here
  authToken: // Add your auth token here
}